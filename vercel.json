{
  "version": 2,
  "builds": [
    {
      "src": "node.js",
      "use": "@vercel/node",
      "config": { "includeFiles": [ "dist/**" ] }
    },
    {
      "src":"public/**/*".
      "use":"@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "node.js"
    }
  ]
}
